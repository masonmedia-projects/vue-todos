<div class=\"\">\n                <h5 class=\"mt-0 mb-1\">{{ todo.title }}</h5>\n                <p class=\"mb-0\">{{ todo.project }}</p>\n            </div>\n        </b-media>\n        <div v-show=\"isEditing\">\n            <b-modal :id=\"todo.id\" centered\n            header-class=\"p-4 border-0\"\n            footer-class=\"p-4 border-0\"\n            header-bg-variant=\"dark-grape\"\n            header-text-variant=\"light\"\n            body-bg-variant=\"dark-grape\"\n            body-text-variant=\"light\"\n            body-class=\"p-5\"\n            footer-bg-variant=\"dark-grape\"\n            footer-text-variant=\"light\"\n            >\n                <p class=\"mb-4\">Edit your todo!</p>\n                <b-input-group class=\"mt-3\">\n                    <template #prepend>\n                        <b-input-group-text>Title</b-input-group-text>\n                    </template>\n                    <b-form-input v-model=\"todo.title\"></b-form-input>\n                </b-input-group>\n                <b-input-group class=\"mt-3\">\n                    <template #prepend>\n                    <b-input-group-text>Project</b-input-group-text>\n                    </template>\n                    <b-form-input v-model=\"todo.project\"></b-form-input>\n                </b-input-group>\n                <template #modal-footer>\n                    <b-button class=\"w-50\" pill variant=\"purple\" @click=\"modalClose\" >Close</b-button>\n                </template>\n            </b-modal>\n        </div>\n    </div>\n\n    \n</template>\n\n<script>\nexport default {\n    name: 'Todo',\n    props: [\n        'todo'\n    ],\n    data() {\n        return {\n            isEditing: false,\n        }\n    },\n    methods: {\n        showForm() {\n            this.isEditing = true;\n        },\n        hideForm() {\n            this.isEditing = false;\n        },\n        deleteTodo(todo) {\n            this.$emit('delete-todo', todo)\n        },\n        modalClose(todo) {\n            this.$emit('modal-close', todo)\n\n        }\n    }\n}\n</script>\n\n<style>\n\n.modal-header {\n    border-top-left-radius: 33px;\n    border-top-right-radius: 33px;\n}\n.modal-footer {\n    border-bottom-left-radius: 33px;\n    border-bottom-right-radius: 33px;\n}\n\n.modal-content {\n    position: relative;\n    display: flex;\n    flex-direction: column;\n    width: 100%;\n    pointer-events: auto;\n    background-color: transparent;\n    background-clip: padding-box;\n    border: 0;\n    border-radius: 0.3rem;\n    outline: 0;\n}\n\n</style>"]}]}